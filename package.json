{
  "name": "aem-genai-assistent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version aio app run",
    "deploy:app": "REACT_APP_VERSION=$npm_package_version aio app deploy --no-actions",
    "deploy:actions": "aio app deploy --no-web-assets",
    "deploy": "aio app deploy",
    "lint": "eslint web-src/src/**/*.js test/**/*.js",
    "fix": "eslint web-src/src/**/*.js test/**/*.js --fix",
    "test": "jest --passWithNoTests ./test",
    "e2e": "jest --collectCoverage=false --testRegex ./e2e"
  },
  "dependencies": {
    "@adobe/aio-sdk": "3.0.0",
    "@adobe/generator-add-action-generic": "0.2.9",
    "@adobe/react-spectrum": "3.31.1",
    "@react-spectrum/toast": "3.0.0-beta.6",
    "@spectrum-icons/illustrations": "3.6.7",
    "@spectrum-icons/workflow": "4.2.6",
    "prismjs": "1.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-simple-code-editor": "0.13.1",
    "uuid": "9.0.1",
    "wretch": "2.7.0"
  },
  "devDependencies": {
    "@adobe/aio-cli": "9.4.1",
    "@adobe/eslint-config-helix": "2.0.2",
    "@babel/core": "7.18.6",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-transform-react-jsx": "7.22.15",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@openwhisk/wskdebug": "1.3.0",
    "@semantic-release/changelog": "6.0.3",
    "eslint": "8.52.0",
    "eslint-plugin-react": "7.33.2",
    "jest": "29"
  },
  "engines": {
    "node": "^14.18 || ^16.13 || >=18"
  },
  "aio-app-builder-templates": [
    "@adobe/generator-add-action-generic"
  ],
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
